        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "f54d9e61-2616-48c1-9636-15302e2ba00b"
      }
    },
    "owner": "bharath14321"
